<%= simple_form_for @plan, :url => {:action => :create}, :html => {:id => "plan_form", :class => "form-horizontal"} do |f| %>
<div class="form-inputs">
  <%= f.input :title, required: true %>
  <%= f.input :user_id, required: true %>
</div>
  <div class="form-actions">
  <%= f.button :submit, "Create Plan" %>
</div>
<% end %>